{
  "name": "Ember-shell",
  "description": "An Ember addon that enables you to have a fully customizable OS-like shell, where you can run your own ember apps powered by ember-engines",
  "url": "http://ember-shell.com/",
  "version": "0.1.2-alpha",
  "options": {
    "paths": [
      "packages/shell-app/lib",
      "packages/shell-bus/lib",
      "packages/shell-debug/lib",
      "packages/shell-desktop/lib",
      "packages/shell-main/lib",
      "packages/shell-registry/lib",
      "packages/shell-security/lib",
      "packages/shell-store/lib",
      "packages/shell-ui/lib"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false
  }
}